    <script>
        $(document).ready(function () {
            let currentLocation = document.location.pathname
            $pageHeader = document.querySelector('#page-header>.col-12.pt-3.pb-3 .card .card-body')
            
            // course admin context menu tweaks
            if (currentLocation === '/course/view.php') {
                let $courseAdminContextMenu = document.querySelector('.context-header-settings-menu')
                $pageHeader.insertAdjacentElement('beforeend', $courseAdminContextMenu) 
            }
    
            // activity navigation tweak
            if ((currentLocation.includes('/mod/') && currentLocation.includes('/view.php')) || currentLocation === '/mod/scorm/player.php' || currentLocation === '/mod/scorm/player.php')  {
                $actNavigation = document.querySelector('.activity-navigation')
                $actNavigation.classList.remove('mt-5')
                $actNavigation.classList.remove('mb-1')
                $actNavigation.style.display = 'initial'
                $pageHeader.insertAdjacentElement('beforeend', $actNavigation) 
    
            }
            $("#next-activity-link").innerHTML = ""
            $("#prev-activity-link").innerHTML = ""
        });
    </script>
    <script>
        $(document).ready(function () {
            // Attatchment selector
            let selector = '.formulation .ablock .attachments a'
    
            // ensure all common web image formats are included
            let imageExtensions = ['png', 'jpg', 'jpe', 'jpeg', 'gif', 'svg', 'svgz']
    
            // If is an image file, put it inline
            $(selector).map(function () {
                let link = this.href.replace('?forcedownload=1', '')
                let extension = link.split('.').pop().toLowerCase()
                if (imageExtensions.includes(extension)) {
                    $(this).replaceWith("<img class='rounded img-responsive' src='" + link + "'" + "alt='inline-image'>")
                }
            })
        })
    </script>
    
    <script>
        $(document).ready(function() {
            $('#page-course-view-mosaics .mosaic-livedate span').map(function(){
                if (this.innerText !== "Revive esta clase") {
                    $(this).addClass("live-green")
                }
                if (this.innerText == "En vivo ahora") {
                    $(this).addClass("live-coral")
                    this.innerText = "En vivo"
                }
            })
        })
    </script>
    
    <script>

    // Hide badges section
    $('section h3').map(function(){
        if(this.innerText == "Insignias") {
            $(this).parent().remove()
        }
    })
    </script>

