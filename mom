    <script>
    // Expose values from tagmanager block as global variables so Google Tagmanager can add them
var tag_vars = document.getElementsByClassName('tagmanager_var');
for(var i = 0; i < tag_vars.length; i++){
    var name = tag_vars[i].getAttribute('data-name');
    if(name){
        window[name] = tag_vars[i].innerText;
    }
}

// Change formatting on firstAccess
var bits = firstAccess.split('/');
firstAccess = "20" + bits[2] + bits[1] + bits[0];
</script>
    
